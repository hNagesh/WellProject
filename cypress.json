{
  "testFiles": "**/*.feature", 
  "reporter": "junit",
   "reporterOptions": {
      "mochaFile": "cypress/downloads/results-[hash].xml",

      "toConsole": true
   },
  
  "chromeWebSecurity": false,
  "defaultCommandTimeout": 100000,
  "pageLoadTimeout": 80000,
  "viewportWidth": 1800,
  "viewportHeight": 1100,
  "video":false,
  "screenshotFolder":"cypress/screenshots",
  "trashAssetsBeforeRuns": true,
  "env":{
    "Enviroment":"Test"
    }
   
}
